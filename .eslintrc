{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jquery": true
  },
  //"extends": [
  //  "prettier",
  //],
  //"plugins": [
  //  "prettier"
  //],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  globals: {
    "$" : true,
    "jQuery" : true,
    "YT" : true,
    "FB" : true,
    "twttr" : true
  },
  "rules": {
    "no-console": 0,
    "no-alert": 0,
    "no-debugger": 1,
    "no-var": 1,
    "no-eq-null": 1,
    "no-implicit-coercion": 1,
    "no-implied-eval": 1,
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "space-before-function-paren": 0,
    //"prettier/prettier": [
    //  "error",
    //  {
    //    "singleQuote": true,
    //    "bracketSpacing": true,
    //    "printWidth": 10000
    //  }
    //]
  }
}
